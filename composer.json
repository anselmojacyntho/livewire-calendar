{
    "name": "anselmojacyntho/livewire-calendar",
    "description": "Laravel Livewire calendar component updated by Andrés Santibáñez",
    "keywords": [
        "anselmojacyntho",
        "livewire-calendar"
    ],
    "homepage": "https://github.com/anselmojacyntho/livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Andrés Santibáñez",
            "email": "santibanez.andres@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "livewire/livewire": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "AnselmoJacyntho\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnselmoJacyntho\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnselmoJacyntho\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "AnselmoJacyntho\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
